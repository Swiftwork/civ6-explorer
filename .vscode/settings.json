{
  "css.validate": false,
  "docthis.includeMemberOfOnClassMembers": false,
  "docthis.includeMemberOfOnInterfaceMembers": false,
  "files.insertFinalNewline": true,
  "html.format.wrapAttributes": "force",
  "postcssSorting": "./config/postcss.sorting.js",
  "scss.validate": false,
  "search.exclude": {
    "**/node_modules": true,
    "dist": true,
    "docs": true
  },
  "stylefmt.config": "./config/stylelint.config.js",
  "stylelint.enable": true,
  "tslint.autoFixOnSave": true,
  "tslint.configFile": "./config/tslint.config.js",
  "tslint.enable": true,
  "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescriptHero.resolver.importGroups": [
    "Plains",
    "Modules",
    "/module|resolver|service/",
    "Workspace"
  ],
  "typescriptHero.resolver.organizeOnSave": true,
