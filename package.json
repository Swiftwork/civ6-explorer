  "private": true,
  "scripts": {
    "start": "rimraf build && webpack-dev-server --config .config/debug.config.js",
    "start:dist": "rimraf build && webpack-dev-server --config .config/dist.config.js",
    "build": "rimraf build/* && webpack --config .config/debug.config.js",
    "build:dist": "rimraf build/* && webpack --config .config/dist.config.js",
    "test": "karma start .config/karma.config.js",
    "test:runner": "karma start .config/karma.config.js --browsers Chrome --auto-watch --no-single-run --reporters kjhtml",
    "docs": "compodoc src/ --tsconfig src/tsconfig.app.json --toggleMenuItems all --theme readthedocks --output docs/"
